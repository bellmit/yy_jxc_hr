<?xml version="1.0" encoding='UTF-8'?>
<ufinterface account="01" billtype="vouchergl" filename="" groupcode="" isexchange="Y" replace="Y" roottag="" sender="V5-V6">
	<voucher id="">
		<voucher_head>
			<!-- 凭证主键  如果没有那就是新增，有就是修改 可空-->
			<pk_voucher></pk_voucher>
			<!--凭证类别 非空   （凭证类别）-->
			<pk_vouchertype>01</pk_vouchertype>
			<!--会计年度 非空-->
			<year></year>
			<!--来源系统 非空  只支持模块编号-->
			<pk_system>GL</pk_system>
			<!--凭证类型值 0：正常凭证 3：数量调整凭证 不可空-->
			<voucherkind>0</voucherkind>
			<!--核算账簿 非空 （账簿_财务核算账簿）-->
			<pk_accountingbook></pk_accountingbook>
			<!--作废标志 可空-->
			<discardflag>N</discardflag>
			<!-- 会计期间 非空-->
			<period></period>
			<!-- 凭证号为空自动分配 非空：按凭证号处理 -->
			<no></no>
			<!-- 附单据数 可空-->
			<attachment>0</attachment>
			<!-- 制单日期 非空-->
			<prepareddate></prepareddate>
			<!-- 制单人 非空  （用户）-->
			<pk_prepared></pk_prepared>
			<!-- 出纳 （用户）-->
			<pk_casher></pk_casher>
			<!-- 签字标志 -->
			<signflag>N</signflag>
			<!-- 审核人 （用户）-->
			<pk_checked></pk_checked>
			<!-- 记账日期 -->
			<tallydate></tallydate>
			<!-- 记账人 （用户）-->
			<pk_manager></pk_manager>
			<!-- 所属组织 非空 （组织） -->
			<pk_org></pk_org>
			<!-- 所属组织版本，可空 （组织版本） -->
			<pk_org_v></pk_org_v>
			<!-- 所集团  如果不输集团取当前登陆集团-->
			<pk_group></pk_group>
			<free1></free1>
			<!-- 凭证分录   -->
			<details>
				<item>
					<!-- 分录号 非空-->
					<detailindex></detailindex>
					<!-- 摘要 非空-->					
					<explanation></explanation>
					<!-- 业务日期 可空-->
					<verifydate></verifydate>
					<!-- 单价 可空-->
					<price>0.00000000</price>
					<!-- 折本汇率 可空-->
					<excrate2>1</excrate2>
					<!-- 借方数量 可空-->
					<debitquantity>0.00000000</debitquantity>
					<!-- 原币借方金额 可空-->
					<debitamount>0.00000000</debitamount>
					<!-- 本币借方金额 可空-->
					<localdebitamount>0.00000000</localdebitamount>
					<!-- 贷方数量 可空-->
					<creditquantity>0.00000000</creditquantity>
					<!-- 原币贷方金额 可空-->
					<creditamount>0.00000000</creditamount>
					<!-- 本币贷方金额 可空-->
					<localcreditamount>0.00000000</localcreditamount>
					<!-- 币种 非空-->
					<pk_currtype>CNY</pk_currtype>
					<!-- 科目 非空 -->
					<pk_accasoa></pk_accasoa>
				</item>
			</details>
		</voucher_head>
	</voucher>
</ufinterface>
